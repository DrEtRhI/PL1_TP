#include <stdio.h>#define TAILLE 100typedef struct {char prenom[20]; int age;} Personne;void creationPersonne(Personne* p, int x, char nom[20]);int main(){		Personne p1;	creationPersonne(&p1, 12, "Paul");	printf("identite personne\nprenom : %s\nage : %d\n", p1.nom, p1.age); return 0;}void creationPersonne(Personne* p, int x, char nom[20]){	(*p).age = x;	(*p).nom = nom[20];}